<librec-auto>
	<random-seed>202110</random-seed>
	<thread-count>1</thread-count>
	<data>
		<data-dir>../../../../data</data-dir>
		<format>UIR</format>
		<data-file format="text">ml_sample_6000.csv</data-file>
	</data>

	
	<features>
		<appender-class>net.librec.data.convertor.appender.ItemFeatureAppender</appender-class>
		<item-feature-file>movieid_feature_df_gender.csv</item-feature-file>
		<protected-feature name="fea:women_writer_director" type="fea:women_writer_director">women_writer_director</protected-feature>
	</features>

	<optimize>
		<iterations>50</iterations>
		<metric>ndcg</metric>
		<previous-max>0.024878</previous-max>
		<optimization-type>multiplicative</optimization-type>
	</optimize>

	
	<splitter>
		<model count="5">kcv</model>
		<save>true</save>
	</splitter>

	
	<alg>
		<class>biasedmf</class>
		<learn-rate>0.001</learn-rate>
	<learn-rate-max>0.01</learn-rate-max>
		<learn-rate-decay>1.0</learn-rate-decay>
		<bold-driver>true</bold-driver>
	    <iterator-max>350</iterator-max>
		<user-reg var="true">0.09852052104634168</user-reg><item-reg var="true">0.19333759897201686</item-reg><bias-reg var="true">0.08567953290868634</bias-reg><num-factors var="true" type="discrete">100</num-factors><early-stop>true</early-stop>
	<similarity type="item">pcc</similarity> --&gt;
		</alg>
	
	<metric>
		<ranking>true</ranking>
		<list-size>50</list-size>
		<class>ndcg,psp</class>
		<protected-feature type="fea:women_writer_director">women_writer_director</protected-feature>
	</metric>


	<rerank>
		<script lang="python3" src="system">
			<script-name>far_rerank.py</script-name>
			<param name="max_len">10</param>
			<param var="true" name="lambda">0.7791418986554667</param><param name="binary">False</param>
			<param name="protected_feature" type="fea:women_writer_director">women_writer_director</param>
		</script>
	</rerank>

	

	<post>
		<script lang="python3" src="system">
			<script-name>result_graphics.py</script-name>
			<param name="browser">true</param>
		</script>
		<script lang="python3" src="system">
			<script-name>results_to_csv.py</script-name>
			<param name="option">all</param>
		</script>
	</post>
<!--This configuration file was automatically generated by librec-auto. Editing may produce unpredictable results and is not recommended.--></librec-auto>
